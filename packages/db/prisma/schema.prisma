generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String        @id         @default(uuid())
  email     String
  website   Websites[]
  service   Service[]
}

model Websites {
  id        String        @id         @default(uuid())
  url       String        @unique
  userId    String
  user      User          @relation(fields: [userId], references: [id])
  service   Service[]
}

model Service {
  id        String        @id         @default(uuid())
  phone     Int           @unique
  email     String        @unique
  websiteId String 
  userId    String

  user      User          @relation(fields: [userId], references: [id])
  website   Websites      @relation(fields: [websiteId], references: [id])
}