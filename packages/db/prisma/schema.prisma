generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String        @id         @default(uuid())
  email     String
  website   Websites[]
}

model Websites {
  id        String        @id         @default(uuid())
  url       String        @unique
  userId    String
  user      User          @relation(fields: [userId], references: [id])
}